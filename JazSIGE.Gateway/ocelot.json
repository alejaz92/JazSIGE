{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/auth/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5172
        }
      ],
      "UpstreamPathTemplate": "/auth/{everything}",
      "UpstreamHttpMethod": [ "POST", "GET", "PUT", "DELETE" ],
      "AddHeadersToRequest": {
        "Authorization": "{Headers.Authorization}"
      },
      "SwaggerKey": "auth"
    },
    {
      "DownstreamPathTemplate": "/api/catalog/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7285
        }
      ],
      "UpstreamPathTemplate": "/catalog/{everything}",
      "UpstreamHttpMethod": [ "POST", "GET", "PUT", "DELETE" ],
      "AddHeadersToRequest": {
        "Authorization": "{Headers.Authorization}"
      },
      "SwaggerKey": "catalog"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "auth",
      "Config": [
        {
          "Name": "AuthService API",
          "Version": "v1",
          "Url": "http://localhost:5172/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "catalog",
      "Config": [
        {
          "Name": "CatalogService API",
          "Version": "v1",
          "Url": "http://localhost:7285/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5290"
  }
}
